#!/usr/bin/python
import csv
import sys

input = file(sys.argv[1])
avrg = 0
grades = {}
numOfSt = 0

print input.next(),

for line in input:
  array = line.split(',')
  try:
    grades[int(array[2])]+=1
  except KeyError:
    grades[int(array[2])]=1

for grade.num in grades.iteritems():
  print str(grade.strip()) + str(num) 
 
